<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT_PLAIN" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d, %-5p, %c:%L, %t] - %mdc %m%n</pattern>
        </encoder>
    </appender>

    <logger name="no.nav.pgi.popp.lagreinntekt" level="DEBUG" />

    <root level="WARN">
        <appender-ref ref="STDOUT_PLAIN" />
    </root>
</configuration>
